{
  "name": "webpack-react",
  "version": "1.0.0",
  "description": "Webpack and react",
  "private": true,
  "jest": {
    "verbose": false,
    "collectCoverageFrom": [
      "**/*.{ts,tsx}",
      "!**/node_modules/**"
    ],
    "setupFilesAfterEnv": [
      "<rootDir>/src/setupTests.ts"
    ],
    "moduleNameMapper": {
      ".+\\.(css|styl|sass|scss|png)$": "identity-obj-proxy"
    },
    "moduleDirectories": [
      "node_modules",
      "src"
    ]
  },
  "scripts": {
    "start": "webpack-dev-server --config webpack.dev.js --open --hot",
    "build": "webpack --config webpack.prod.js",
    "test": "jest --coverage --colors",
    "deploy": "gh-pages -d dist",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Natalka2019/webpack-react.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Natalka2019/webpack-react/issues"
  },
  "homepage": "https://github.com/Natalka2019/webpack-react",
